<div class="row" style="margin-top: .5%">
    <div class="col s8"><span class="flow-text">
        <h4 class="center">Welcome!</h4><hr>
    </span>
    <div class="row">
        <div class="col s12">
            <table class="striped">
                    <thead>
                        <tr>
                            <th>Category</th>
                            <th>Activity</th>
                            <th>Rating</th>
                            <th>Price Range</th>
                            {{!-- <th></th> --}}
                        </tr>
                    </thead>
                    <tbody>
                        {{#each activities}}
                            <tr>
                                <td> {{ categories }} </td>
                                <td> {{ activity }} </td>
                                <td> {{ rating }} </td>
                                <td> ${{ cost }} </td>
                            {{!-- <td><a href="#" class="right">More Info</a></td> --}}
                            </tr>
                        {{/each}}
                    </tbody>
                    </table>
        </div>
    </div>
    </div>
        <div class="col s4"><span class="flow-text">
            <h4 class="center">Budgeting</h4><hr>
            </span>
                <div class="row">
                <div class="col s12">
                    <table class="striped">
                        <thead>
                        <tr>
                            <th>Description</th>
                            <th>Cost</th>
                            {{!-- <th>Remaining</th> --}}
                        </tr>
                        </thead>
                        <tbody>
                        {{#each budgets}}
                        <tr>
                            <td>{{ description }}</td>
                            <td>${{ cost }}</td>
                        </tr>
                        {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

